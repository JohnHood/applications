#%Module1.0

proc ModulesHelp { } {
  puts stderr "HDF5 1.8.11 compiled with Intel Cluster Studio XE 2013 (STATIC) MPI version"
  puts stderr "TAGS: hdf5"
  puts stderr "URL: http://www.hdfgroup.org/HDF5/"
}

module-whatis "HDF5 1.8.11 compiled with Intel Cluster Studio XE 2013 MPI Version"

module load zlib/1.2.5_intel-2013-sandybridge

set root     /share/libs/hdf5/1.8.11/sandybridge/ics-2013/impi-4.1.0.024
prepend-path LD_LIBRARY_PATH $root/lib
prepend-path CPATH           $root/include
prepend-path PATH            $root/bin

