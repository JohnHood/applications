#%Module1.0

module-whatis "PHENIX is a software suite for the automated determination of macromolecular structures using X-ray crystallography and other methods."

set fsldir /share/apps/fsl
set phenix /share/apps/phenix/phenix-1.8.1-1168
setenv PHENIX $phenix
setenv PHENIX_INSTALLER_DATE 280920121648
setenv PHENIX_VERSION 1.8.1
setenv PHENIX_RELEASE_TAG 1168
setenv PHENIX_ENVIRONMENT 1
setenv PHENIX_MTYPE intel-linux-2.6-x86_64
setenv PHENIX_MVERSION redhat-e6.1
setenv PHENIX_USE_MTYPE intel-linux-2.6-x86_64

prepend-path PATH $phenix/build/intel-linux-2.6-x86_64/bin
set shell [module-info shell]




