#%Module

proc ModulesHelp { } {
    puts stderr {   SWIG is a software development tool that connects programs written in C and C++ with
 a variety of high-level programming languages. - Homepage: http://www.swig.org/
    }
}

module-whatis {Description: SWIG is a software development tool that connects programs written in C and C++ with
 a variety of high-level programming languages. - Homepage: http://www.swig.org/}

set root    /share/easybuild/RHEL6.3/westmere/software/SWIG/2.0.4-ictce-5.4.0-Python-2.7.3

conflict SWIG

if { ![is-loaded ictce/5.4.0] } {
    module load ictce/5.4.0
}

if { ![is-loaded Python/2.7.3-ictce-5.4.0] } {
    module load Python/2.7.3-ictce-5.4.0
}

if { ![is-loaded PCRE/8.12-ictce-5.4.0] } {
    module load PCRE/8.12-ictce-5.4.0
}

prepend-path	MANPATH		$root/share/man
prepend-path	PATH		$root/bin

setenv	EBROOTSWIG		"$root"
setenv	EBVERSIONSWIG		"2.0.4"
setenv	EBDEVELSWIG		"$root/easybuild/SWIG-2.0.4-ictce-5.4.0-Python-2.7.3-easybuild-devel"


