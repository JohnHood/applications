#%Module1.0

module-whatis "NetCDF Serial 4.3.0 compiled with Intel Cluster Studio XE 2013, HDF5 1.8.11 and GPFS (STATIC)"

module load hdf5/1.8.11_intel-2013-serial-sandybridge

set netcdfroot /share/libs/netcdf/4.3.0/sandybridge/ics-2013/hdf5-1.8.11/serial

setenv       NETCDF $netcdfroot
prepend-path PATH $netcdfroot/bin
prepend-path CPATH $netcdfroot/include
prepend-path LD_LIBRARY_PATH $netcdfroot/lib
prepend-path MANPATH $netcdfroot/share/man
prepend-path INFOPATH $netcdfroot/share/info
