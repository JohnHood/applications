#%Module

proc ModulesHelp { } {
    puts stderr {   Sequence assembler for very short reads - Homepage: http://www.ebi.ac.uk/~zerbino/velvet/
    }
}

module-whatis {Sequence assembler for very short reads - Homepage: http://www.ebi.ac.uk/~zerbino/velvet/}

set root    /share/easybuild/RHEL6.3/westmere/software/Velvet/1.2.10-goolf-1.6.10

conflict    Velvet

if { ![is-loaded goolf/1.6.10] } {
    module load goolf/1.6.10
}

prepend-path	PATH		$root/bin

setenv	EBROOTVELVET		"$root"
setenv	EBVERSIONVELVET		"1.2.10"
setenv	EBDEVELVELVET		"$root/easybuild/Velvet-1.2.10-goolf-1.6.10-easybuild-devel"


